{
  "name": "react-vapor-demo",
  "version": "24.13.5",
  "private": true,
  "description": "Vapor Design System",
  "main": "src/Index.ts",
  "scripts": {
    "start": "tsc --build && concurrently \"tsc --build --watch\" \"webpack-dev-server\"",
    "build": "tsc --build && webpack",
    "precommit": "lint-staged",
    "snyk-protect": "npx snyk protect",
    "prepublish": "npm run snyk-protect",
    "clean": "rimraf dist built",
    "lintfix": "../../node_modules/.bin/stylelint --fix \"**/*.scss\" && ../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint -c ./.eslintrc.js \"src/**/*.{ts,tsx}\" --fix"
  },
  "author": "Coveo",
  "license": "Apache-2.0",
  "homepage": "https://vapor.cloud.coveo.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/react-vapor.git"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "codemirror": "^5.58.2",
    "coveo-styleguide": "^24.13.5",
    "d3": "3.5.17",
    "faker": "4.1.0",
    "jquery": "^3.5.1",
    "lorem-ipsum": "2.0.3",
    "moment": "^2.27.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-markdown": "5.0.3",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "15.4.4",
    "react-vapor": "^24.13.5",
    "redux": "4.1.1",
    "redux-devtools-extension": "2.13.9",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "underscore": "1.13.1",
    "underscore.string": "3.3.5"
  },
  "devDependencies": {
    "@loadable/component": "5.10.2",
    "@types/classnames": "2.2.9",
    "@types/faker": "4.1.9",
    "@types/loadable__component": "5.10.0",
    "@types/lorem-ipsum": "2.0.0",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/redux-promise-middleware": "6.0.0",
    "@types/underscore": "1.8.10",
    "@types/webpack-env": "1.15.1",
    "concurrently": "^6.0.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "3.4.2",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "4.5.1",
    "html-webpack-tags-plugin": "^2.0.17",
    "lint-staged": "9.4.2",
    "node-sass": "5.0.0",
    "raw-loader": "4.0.0",
    "rimraf": "^3.0.2",
    "sass": "1.25.0",
    "sass-loader": "10.1.0",
    "snyk": "^1.158.0",
    "source-map-loader": "1.1.0",
    "style-loader": "1.1.3",
    "tslib": "^1",
    "typescript": "3.8.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.2"
  },
  "lint-staged": {
    "**/*.scss": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint -c ./.eslintrc.js --fix",
      "git add"
    ],
    "**/*.{js,jsx,json,md,yml,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "snyk": true
}
